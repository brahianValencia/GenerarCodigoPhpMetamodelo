private String getDiagramClassba_1000Text(org.eclipse.gmf.runtime.notation.View view) {
			concreta.DiagramClassba domainModelElement = (concreta.DiagramClassba) view.getElement();
if (domainModelElement != null) {
	return domainModelElement.getName();
} else {
	concreta.diagram.part.ConcretaDiagramEditorPlugin.getInstance().logError("No domain element for view with visualID = " + 1000);  //$NON-NLS-1$
		return "";  //$NON-NLS-1$
}
	}
