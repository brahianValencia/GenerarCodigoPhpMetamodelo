 */
public class ConcretaVisualIDRegistry {

		/**
 * @generated
 */
	private static final String DEBUG_KEY = "dc_concreta.diagram/debug/visualID";  //$NON-NLS-1$

		/**
 * @generated
 */
public static int getVisualID(org.eclipse.gmf.runtime.notation.View view) {
	if (view instanceof org.eclipse.gmf.runtime.notation.Diagram) {
		if (concreta.diagram.edit.parts.DiagramClassbaEditPart.MODEL_ID.equals(view.getType())) {
			return concreta.diagram.edit.parts.DiagramClassbaEditPart.VISUAL_ID;
		} else {
			return -1;
		}
	}
	return concreta.diagram.part.ConcretaVisualIDRegistry.getVisualID(view.getType());
}
	
		/**
 * @generated
 */
public static String getModelID(org.eclipse.gmf.runtime.notation.View view) {
	org.eclipse.gmf.runtime.notation.View diagram = view.getDiagram();
	while (view != diagram) {
		org.eclipse.emf.ecore.EAnnotation annotation = view.getEAnnotation("Shortcut");  //$NON-NLS-1$
		if (annotation != null) {
			return (String) annotation.getDetails().get("modelID");  //$NON-NLS-1$
		}
		view = (org.eclipse.gmf.runtime.notation.View) view.eContainer();
	}
	return diagram != null ? diagram.getType() : null;
}
	
		/**
 * @generated
 */
public static int getVisualID(String type) {
	try {
		return Integer.parseInt(type);
	} catch (NumberFormatException e) {
		if (Boolean.TRUE.toString().equalsIgnoreCase(org.eclipse.core.runtime.Platform.getDebugOption(DEBUG_KEY))) {
			concreta.diagram.part.ConcretaDiagramEditorPlugin.getInstance().logError("Unable to parse view type as a visualID number: " + type);
		}
	}
	return -1;
}

	/**
 * @generated
 */
public static String getType(int visualID) {
	return Integer.toString(visualID);
}
	
		/**
 * @generated
 */
public static int getDiagramVisualID(org.eclipse.emf.ecore.EObject domainElement) {
	if (domainElement == null) {
		return -1;
	}
	if (concreta.ConcretaPackage.eINSTANCE.getDiagramClassba().isSuperTypeOf(domainElement.eClass()) && isDiagram((concreta.DiagramClassba) domainElement)) {
	return concreta.diagram.edit.parts.DiagramClassbaEditPart.VISUAL_ID;
}
	return -1;
}
	
		/**
 * @generated
 */
public static int getNodeVisualID(org.eclipse.gmf.runtime.notation.View containerView, org.eclipse.emf.ecore.EObject domainElement) {
	if (domainElement == null) {
		return -1;
	}
	String containerModelID = concreta.diagram.part.ConcretaVisualIDRegistry.getModelID(containerView);
	if (!concreta.diagram.edit.parts.DiagramClassbaEditPart.MODEL_ID.equals(containerModelID) && !"concreta".equals(containerModelID)) {  //$NON-NLS-1$
		return -1;
	}
	int containerVisualID;
	if (concreta.diagram.edit.parts.DiagramClassbaEditPart.MODEL_ID.equals(containerModelID)) {
		containerVisualID = concreta.diagram.part.ConcretaVisualIDRegistry.getVisualID(containerView);
	} else {
		if (containerView instanceof org.eclipse.gmf.runtime.notation.Diagram) {
			containerVisualID = concreta.diagram.edit.parts.DiagramClassbaEditPart.VISUAL_ID;		
		} else {
			return -1;
		}
	}
	switch (containerVisualID) {
			case concreta.diagram.edit.parts.DiagramClassbaEditPart.VISUAL_ID:
	if (concreta.ConcretaPackage.eINSTANCE.getClassba().isSuperTypeOf(domainElement.eClass())) {
	return concreta.diagram.edit.parts.ClassbaEditPart.VISUAL_ID;
}
	break;
					case concreta.diagram.edit.parts.ClassbaClassbaLstAttributesbaCompartmentEditPart.VISUAL_ID:
	if (concreta.ConcretaPackage.eINSTANCE.getAttributeba().isSuperTypeOf(domainElement.eClass())) {
	return concreta.diagram.edit.parts.AttributebaEditPart.VISUAL_ID;
}
	break;
		case concreta.diagram.edit.parts.ClassbaClassbaLstMethodbaCompartmentEditPart.VISUAL_ID:
	if (concreta.ConcretaPackage.eINSTANCE.getMethodba().isSuperTypeOf(domainElement.eClass())) {
	return concreta.diagram.edit.parts.MethodbaEditPart.VISUAL_ID;
}
	break;
		}
	return -1;
}
	
		/**
 * @generated
 */
public static boolean canCreateNode(org.eclipse.gmf.runtime.notation.View containerView, int nodeVisualID) {
	String containerModelID = concreta.diagram.part.ConcretaVisualIDRegistry.getModelID(containerView);
	if (!concreta.diagram.edit.parts.DiagramClassbaEditPart.MODEL_ID.equals(containerModelID) && !"concreta".equals(containerModelID)) {  //$NON-NLS-1$
		return false;
	}
	int containerVisualID;
	if (concreta.diagram.edit.parts.DiagramClassbaEditPart.MODEL_ID.equals(containerModelID)) {
		containerVisualID = concreta.diagram.part.ConcretaVisualIDRegistry.getVisualID(containerView);
	} else {
		if (containerView instanceof org.eclipse.gmf.runtime.notation.Diagram) {
			containerVisualID = concreta.diagram.edit.parts.DiagramClassbaEditPart.VISUAL_ID;		
		} else {
			return false;
		}
	}
	switch (containerVisualID) {
		case concreta.diagram.edit.parts.DiagramClassbaEditPart.VISUAL_ID:
	if (concreta.diagram.edit.parts.ClassbaEditPart.VISUAL_ID == nodeVisualID) {
	return true;
}
	break;
case concreta.diagram.edit.parts.ClassbaEditPart.VISUAL_ID:
	if (concreta.diagram.edit.parts.ClassbaNameEditPart.VISUAL_ID == nodeVisualID) {
	return true;
}
if (concreta.diagram.edit.parts.ClassbaClassbaLstAttributesbaCompartmentEditPart.VISUAL_ID == nodeVisualID) {
	return true;
}
if (concreta.diagram.edit.parts.ClassbaClassbaLstMethodbaCompartmentEditPart.VISUAL_ID == nodeVisualID) {
	return true;
}
	break;
case concreta.diagram.edit.parts.AttributebaEditPart.VISUAL_ID:
	if (concreta.diagram.edit.parts.AttributebaNameEditPart.VISUAL_ID == nodeVisualID) {
	return true;
}
	break;
case concreta.diagram.edit.parts.MethodbaEditPart.VISUAL_ID:
	if (concreta.diagram.edit.parts.MethodbaNameEditPart.VISUAL_ID == nodeVisualID) {
	return true;
}
	break;
case concreta.diagram.edit.parts.ClassbaClassbaLstAttributesbaCompartmentEditPart.VISUAL_ID:
	if (concreta.diagram.edit.parts.AttributebaEditPart.VISUAL_ID == nodeVisualID) {
	return true;
}
	break;
case concreta.diagram.edit.parts.ClassbaClassbaLstMethodbaCompartmentEditPart.VISUAL_ID:
	if (concreta.diagram.edit.parts.MethodbaEditPart.VISUAL_ID == nodeVisualID) {
	return true;
}
	break;
			}
	return false;
}
	
