	public static boolean isSemanticLeafVisualID(int visualID) {
		switch (visualID) {
						case concreta.diagram.edit.parts.DiagramClassbaEditPart.VISUAL_ID:
				return false;
									case concreta.diagram.edit.parts.AttributebaEditPart.VISUAL_ID:case concreta.diagram.edit.parts.MethodbaEditPart.VISUAL_ID:
				return true;
									default:
				break;
		}
		return false;
	}

